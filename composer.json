{
  "name": "darkghosthunter/transbank-api",
  "description": "Transbank SDK no oficial para PHP 7.1+",
  "keywords": [
    "payments",
    "transbank",
    "api",
    "sdk",
    "wrapper"
  ],
  "authors": [
    {
      "name": "Italo Baeza C",
      "email": "yo@italobc.com",
      "homepage": "http://italobc.com",
      "role": "Developer"
    }
  ],
  "version": "@dev",
  "require": {
    "php": ">=7.1.3",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-dom": "*",
    "ext-soap": "*",
    "luisurrutia/transbank-soap": "0.0.1",
    "guzzle/guzzle": "3.9.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "~1.2.0",
    "php-coveralls/php-coveralls": "2.1.*",
    "squizlabs/php_codesniffer": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "DarkGhostHunter\\TransbankApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\" : "tests"
    }
  },
  "license": "MIT"
