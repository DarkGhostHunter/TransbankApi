{
  "name": "darkghosthunter/transbank-wrapper",
  "description": "Wrapper no-oficial para Transbank SDK",
  "keywords": [
    "payments",
    "transbank",
    "api",
    "sdk",
    "wrapper"
  ],
  "version": "0.5",
  "require": {
    "php": ">=7.1.3",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-dom": "*",
    "ext-soap": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "php-coveralls/php-coveralls": "2.1.*",
    "squizlabs/php_codesniffer": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Transbank\\Wrapper": "src",
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests" : "tests"
    }
  },
  "license": "BSD-3-Clause"
}
